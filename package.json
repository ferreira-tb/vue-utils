{
  "name": "@tb-dev/vue",
  "version": "2.2.1",
  "description": "Vue utilities",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@10.13.1",
  "funding": "https://github.com/sponsors/ferreira-tb",
  "author": {
    "name": "Andrew Ferreira",
    "email": "andrew.shien2@gmail.com",
    "url": "https://github.com/ferreira-tb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/vue-utils.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/vue-utils/issues"
  },
  "keywords": [
    "vue",
    "utils",
    "composables"
  ],
  "scripts": {
    "build": "vite build",
    "docs": "typedoc --plugin typedoc-plugin-mdn-links",
    "format": "prettier . --write",
    "lint": "eslint . --config eslint.config.js",
    "release": "pnpm run build && pnpm publish",
    "type-check": "tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@tb-dev/utils": "^7.0.8",
    "@vueuse/core": "^13.5.0",
    "es-toolkit": "^1.39.7",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^8.0.3",
    "@types/node": "^24.1.0",
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.7",
    "typedoc-plugin-mdn-links": "^5.0.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}
